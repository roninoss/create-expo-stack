{
  "name": "rn-new",
  "version": "2.20.1",
  "description": "Shorthand for create-expo-stack",
  "repository": {
    "type": "git",
    "url": "https://github.com/roninoss/create-expo-stack.git",
    "directory": "packages/rn-new"
  },
  "homepage": "https://rn.new",
  "license": "MIT",
  "bin": "./bin/rn-new.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "bun run clean && bun run compile && bun run copy-ai-interface",
    "clean": "rm -rf ./bin ./lib",
    "compile": "mkdir -p bin && cp src/rn-new.js bin/rn-new.js && chmod +x bin/rn-new.js",
    "copy-ai-interface": "mkdir -p lib && cp src/ai-interface.js lib/ai-interface.js",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "create-expo-stack": "^2.20.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": false
  }
}
