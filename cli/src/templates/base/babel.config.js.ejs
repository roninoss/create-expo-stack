module.exports = function(api) {
  api.cache(true);
  let plugins = [];

  <% if (props.stylingPackage?.name === "unistyles") { %>
    plugins.push([
      'react-native-unistyles/plugin',
      {
        autoProcessRoot: 'app',
        autoProcessImports: ['~/components']
      }
    ]);
  <% } %>

  plugins.push('react-native-worklets/plugin');

  return {
    presets: ['babel-preset-expo'],
    plugins,
  };
};
